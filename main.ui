   <property name="styleSheet">
    <string notr="true">QCalendarWidget QWidget {
    alternate-background-color: rgb(228, 220, 215); /* Light beige background */
	
	font: 12pt &quot;MS Shell Dlg 2&quot;;
}

/* style for top navigation area ###############################################*/ 

#qt_calendar_navigationbar {
    background-color: rgb(190, 90, 140); /* White background */
	border: 2px solid  rgb(228, 220, 215); /* Light beige border */
	border-bottom: 0px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	
}

/* style for month change buttons ############################################ */

#qt_calendar_prevmonth, 
#qt_calendar_nextmonth {
    border: none;  /* Remove border */
    qproperty-icon: none; /* Remove default icons */
    min-width: 13px;
    max-width: 13px;
    min-height: 13px;
    max-height: 13px;
    border-radius: 5px; 
    background-color: transparent; /* Set background transparent */
    padding: 5px;
}

/* style for pre month button ############################################ */

#qt_calendar_prevmonth {
	margin-left:5px;
	image: url(:/icon/arrow-119-48.ico); /* Set image */
}

/* style for next month button ########################################### */
#qt_calendar_nextmonth {
	margin-right:5px;
	image: url(:/icon/arrow-19-48.ico); /* Set image */
}

#qt_calendar_prevmonth:hover, 
#qt_calendar_nextmonth:hover {
    background-color: rgb(85, 170, 255); /* Light blue background */
}

#qt_calendar_prevmonth:pressed, 
#qt_calendar_nextmonth:pressed {
    background-color: rgba(235, 235, 235, 1); /* Light gray background */
}


/* Style for month and year buttons #################################### */

#qt_calendar_yearbutton {
    color: rgb(228, 220, 215); /* Black font color */
	margin:5px;
    border-radius: 5px;
	font-size: 13pt;
	padding:0px 10px;
}

 #qt_calendar_monthbutton {
	width: 110px;
    color: rgb(228, 220, 215); /* Black font color */
	font-size: 13pt;
	margin:5px 0px;
    border-radius: 5px;
	padding:0px 2px;
}

#qt_calendar_yearbutton:hover, 
#qt_calendar_monthbutton:hover {
    background-color: rgb(85, 170, 255); /* Light blue background */
	font-size: 13pt;
}

#qt_calendar_yearbutton:pressed, 
#qt_calendar_monthbutton:pressed {
    background-color: rgba(235, 235, 235, 1); /* Light gray background */
	font-size: 13pt;
}

/* Style for year input lineEdit ######################################*/

#qt_calendar_yearedit {
    min-width: 53px;
    color: rgb(0, 0, 0); /* Black font color */
    background: transparent;
	font-size: 13pt;
}

/* Style for year change buttons ######################################*/

#qt_calendar_yearedit::up-button { 
	image: url(:/icon/arrow-151-48.ico);
    subcontrol-position: right;
}

#qt_calendar_yearedit::down-button { 
	image: url(:/icon/arrow-213-48.ico);
    subcontrol-position: left; 
}

#qt_calendar_yearedit::down-button, 
#qt_calendar_yearedit::up-button {
	width:10px;
	padding: 0px 5px;
	border-radius:3px;
}

#qt_calendar_yearedit::down-button:hover, 
#qt_calendar_yearedit::up-button:hover {
	background-color: rgb(85, 170, 255); /* Light blue background */
}

/* Style for month select menu ##################################### */

#calendarWidget QToolButton QMenu {
     background-color: rgb(255, 255, 255); /* White background */
}
#calendarWidget QToolButton QMenu::item {
	/*padding: 10px;*/
}
 #calendarWidget QToolButton QMenu::item:selected:enabled {
    background-color: rgb(85, 170, 255); /* Light blue background */
}

#calendarWidget QToolButton::menu-indicator {
	nosubcontrol-origin: margin;
	subcontrol-position: right center;
	margin-top: 10px;
	width:20px;
}

/* Style for calendar table ########################################## */
#qt_calendar_calendarview {
    outline: 0px; /* Remove the selected dashed box */
	border: 2px solid  rgb(228, 220, 215); /* Light beige border */
	border-top: 0px;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
}

#qt_calendar_calendarview::item:hover {
    border-radius:5px;
	background-color:rgb(170, 255, 255); /* Light blue background */
}

#qt_calendar_calendarview::item:selected {
    background-color: rgb(85, 170, 127); /* Dark green background */
	border-radius:5px;
}
</string>
